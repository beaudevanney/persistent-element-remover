{
  "author": "igorlogius",
  "browser_action": {
    "theme_icons": [
      {
        "light": "dark-256.png",
        "dark": "light-256.png",
        "size": 256
      },
      {
        "light": "dark-128.png",
        "dark": "light-128.png",
        "size": 128
      },
      {
        "light": "dark-64.png",
        "dark": "light-64.png",
        "size": 64
      },
      {
        "light": "dark-32.png",
        "dark": "light-32.png",
        "size": 32
      },
      {
        "light": "dark-16.png",
        "dark": "light-16.png",
        "size": 16
      }
    ]
  },
  "background": {
	"scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Persistent Element Remover",
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "version": "1.8.7"
}
